#include <stdio.h>

int
main(
     int argc,
     char* argv[]
     )
{
  int size = 10;
  int as[size][size];
  
  FILE* file = fopen("output.dat", "r");

  for ( int i = 0; i < size; ++i)
      fread( (void*)as[i], sizeof(int), size, file);
  fclose(file);

   
  for ( int i = 0; i < size; ++i){
    for ( int j = 0; j < size; ++j)
      printf("%i ",as[i][j]);
    printf("\n");
  }


    
  return 0;
}
